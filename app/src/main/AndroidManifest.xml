<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo0"
        android:label="Vedu"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MySafety" >
        <activity
            android:name="com.ved.Vedu.Learn"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.admin"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.SplashScreenActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ved.Vedu.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.SoS"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR_API_KEY" />

        <activity
            android:name="com.ved.Vedu.embassy"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.near"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.number"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.Complaint"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.SignupActivity"
            android:exported="false" />
        <activity
            android:name="com.ved.Vedu.MainActivity"
            android:exported="true" />
    </application>

</manifest>